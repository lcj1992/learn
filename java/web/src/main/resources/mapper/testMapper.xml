<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="dao.TestDao">

    <insert id="saveTest" parameterType="model.TestModel" useGeneratedKeys="true" keyProperty="id">
        insert into name(packageId) values(#{packageId})
    </insert>

    <delete id="deleteTestById">
        delete from name where id = #{id};
    </delete>

</mapper>
